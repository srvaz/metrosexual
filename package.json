{
	"name": "metrosexual",
	"displayName": "metrosexual",
	"description": "Metrosexual is a extension that change your theme based on local hour.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.metrosexualStart",
		"onCommand:extension.metrosexualStop",
		"onCommand:extension.metrosexualLightTheme",
		"onCommand:extension.metrosexualDarkTheme"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.metrosexualStart",
				"title": "Metrosexual: Start Metrosexual"
			},
			{
				"command": "extension.metrosexualStop",
				"title": "Metrosexual: Stop Metrosexual"
			},
			{
				"command": "extension.metrosexualLightTheme",
				"title": "Metrosexual: Define light theme"
			},
			{
				"command": "extension.metrosexualDarkTheme",
				"title": "Metrosexual: Define dark theme"
			}
		],
		"configuration": {
			"title": "Metrosexual",
			"properties": {
				"metrosexual.lightTheme": {
					"type": "string",
					"default": "Default Light+",
					"description": "Sets light theme"
				},
				"metrosexual.darkTheme": {
					"type": "string",
					"default": "Default Dark+",
					"description": "Sets light theme"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
